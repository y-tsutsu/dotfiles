// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    {
        "key": "ctrl+space",
        "command": "git.stage",
        "when": "diffEditorHasChanges || sideBarFocus && activeViewlet == workbench.view.scm"
    },
    {
        "key": "ctrl+shift+space",
        "command": "git.unstage",
        "when": "diffEditorHasChanges || sideBarFocus && activeViewlet == workbench.view.scm"
    },
    {
        "key": "shift+enter",
        "command": "findThenJump.initiate",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+enter",
        "command": "findThenJump.initiateWithSelection",
        "when": "editorTextFocus"
    }
]